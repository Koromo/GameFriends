Parameters {
    float4 ambient;
    float4 diffuse;
    float4 emissive;
    float4 specular;
    float power;
}

Shaders {
    Ref vs {
	path = media/fbxbasic.hlsl;
	entry = vsMain;
        map(_World, world);
        map(_View, view);
        map(_Proj, proj);
    }

    Ref ps {
	path = media/fbxbasic.hlsl;
	entry = psMain_0;
        map(diffuse, diffuse);
    }
}

Pass 0 {
    vertexShader = vs;
    pixelShader = ps;
}